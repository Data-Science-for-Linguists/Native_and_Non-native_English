          <a:documentation>specifies the new state of the paralinguistic feature specified.</a:documentation>
          <choice>
            <value>a</value>
            <a:documentation/>
            <value>f</value>
            <a:documentation/>
            <value>high</value>
            <a:documentation/>
            <value>imitating</value>
            <a:documentation/>
            <value>l</value>
            <a:documentation/>
            <value>laugh</value>
            <a:documentation/>
            <value>neutral</value>
            <a:documentation/>
            <value>not</value>
            <a:documentation/>
            <value>p</value>
            <a:documentation/>
            <value>phone</value>
            <a:documentation/>
            <value>reading</value>
            <a:documentation/>
            <value>reading_aloud</value>
            <a:documentation/>
            <value>sigh</value>
            <a:documentation/>
            <value>singing</value>
            <a:documentation/>
            <value>speaking</value>
            <a:documentation/>
            <value>voice</value>
            <a:documentation/>
            <value>whisp</value>
            <a:documentation/>
            <value>with</value>
            <a:documentation/>
            <value>yawning</value>
            <a:documentation/>
            <value>speaking_with_a_high_voice</value>
            <a:documentation/>
          </choice>
        </attribute>
      </optional>
      <ref name="att.global.attribute.xmlid"/>
      <ref name="att.global.attribute.xmlspace"/>
      <ref name="att.global.linking.attributes"/>
      <ref name="att.global.analytic.attributes"/>
      <ref name="att.global.facs.attributes"/>
