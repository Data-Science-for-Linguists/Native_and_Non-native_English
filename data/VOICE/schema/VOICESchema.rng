  <define name="surplus">
    <element name="surplus">
      <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"> (Texte superflu) marks text present in the source which the editor believes to
  be superfluous or redundant.</a:documentation>
      <ref name="macro.paraContent"/>
      <ref name="att.global.attributes"/>
      <ref name="att.editLike.attributes"/>
      <optional>
        <attribute name="reason">
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">indicates the grounds for believing this text to be superfluous.</a:documentation>
          <list>
            <ref name="data.word"/>
            <zeroOrMore>
              <ref name="data.word"/>
            </zeroOrMore>
          </list>
        </attribute>
      </optional>
      <empty/>
    </element>
  </define>
  <define name="track">
    <element name="track" ns="http://www.univie.ac.at/voice/ns/1.0">
      <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">indicating the beginning of a new track.</a:documentation>
      <empty/>
      <ref name="att.typed.attributes"/>
      <optional>
        <attribute name="medium">
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
          <ref name="data.key"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="num">
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
          <ref name="data.key"/>
        </attribute>
      </optional>
      <empty/>
    </element>
  </define>
  <define name="pvc">
    <element name="pvc" ns="http://www.univie.ac.at/voice/ns/1.0">
      <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Indicating a Pronunciation Variation and Coinage as described in the VOICE Transcription Conventions</a:documentation>
      <ref name="macro.paraContent"/>
      <optional>
        <attribute name="comment">
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
          <text/>
        </attribute>
      </optional>
      <optional>
        <attribute name="ipa" ns="http://www.univie.ac.at/voice/ns/1.0">
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"> Transcription of the item in the International
              Phonetic Alphabet (IPA) </a:documentation>
          <list>
            <data type="token"/>
            <zeroOrMore>
              <data type="token"/>
            </zeroOrMore>
          </list>
        </attribute>
      </optional>
      <empty/>
    </element>
  </define>
  <define name="to">
    <element name="to" ns="http://www.univie.ac.at/voice/ns/1.0">
      <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Indicating a stretch of speach directed to a person</a:documentation>
      <ref name="macro.paraContent"/>
      <ref name="att.segLike.attributes"/>
      <ref name="att.ascribed.attributes"/>
      <empty/>
    </element>
  </define>
  <start>
    <choice>
      <ref name="TEI"/>
      <ref name="teiCorpus"/>
    </choice>
  </start>
</grammar>
