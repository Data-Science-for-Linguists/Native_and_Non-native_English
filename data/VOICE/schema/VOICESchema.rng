      <ref name="att.global.attribute.xmlspace"/>
      <ref name="att.global.linking.attributes"/>
      <ref name="att.global.analytic.attributes"/>
      <ref name="att.global.facs.attributes"/>
      <ref name="att.timed.attributes"/>
      <empty/>
    </element>
  </define>
  <define name="shift">
