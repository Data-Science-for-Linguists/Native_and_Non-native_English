      <body>
         <div type="medium" voice:end="01:46" voice:end_medium_number="1" voice:end_track="8" voice:medium_type="md" voice:start="00:02" voice:start_medium_number="1" voice:start_track="1">
            <u who="#PBsve426_S1" xml:id="PBsve426_u_1"> hello<c function="fall" type="intonation"/></u>
            <u who="#PBsve426_S2" xml:id="PBsve426_u_2"><foreign type="L1" voice:translation="hello/dis" xml:lang="ger"><unclear> gruess gott </unclear></foreign><pause dur="PT1S"/></u>
            <u who="#PBsve426_S1" xml:id="PBsve426_u_3"> e<c type="lengthening"/>r i want to <pause/> prolong to<c type="lengthening"/> stay<c function="fall" type="intonation"/><pause dur="PT3S"/> until two thousand six <pause dur="PT2S"/></u>
            <u who="#PBsve426_S2" xml:id="PBsve426_u_4"> until the end of june or until the end of december<c function="fall" type="intonation"/> two thousand and six<c function="fall" type="intonation"/><anchor synch="#PBsve426_oc_1" type="other_continuation" xml:id="PBsve426_oc_0"/></u>
            <u who="#PBsve426_S1" xml:id="PBsve426_u_5"><anchor synch="#PBsve426_oc_0" type="other_continuation" xml:id="PBsve426_oc_1"/> september <supplied reason="unintelligible"> x<c function="fall" type="intonation"/></supplied><pause dur="PT1S"/></u>
            <u who="#PBsve426_S2" xml:id="PBsve426_u_6"> so just one year<c function="fall" type="intonation"/></u>
            <u who="#PBsve426_S1" xml:id="PBsve426_u_7"> yes just one year<c function="fall" type="intonation"/><pause dur="PT226S"/><incident voice:desc="S2 works on the computer, calculates something; very loud background noises"/><voice:track medium="1" num="02" type="md"/></u>
            <u who="#PBsve426_S2" xml:id="PBsve426_u_8"> have you paid another deposit in <supplied reason="anonymization"> [place1] </supplied><c function="rise" type="intonation"/><pause dur="PT1S"/></u>
            <u who="#PBsve426_S1" xml:id="PBsve426_u_9"> e<c type="lengthening"/>rm deposit<c function="rise" type="intonation"/></u>
            <u who="#PBsve426_S2" xml:id="PBsve426_u_10"> mhm <pause/><seg n="1" type="overlap" xml:id="PBsve426_ol_0"> i guess you paid </seg> another one in <supplied reason="anonymization"> [place1] </supplied><c function="fall" type="intonation"/><pause/></u>
            <u who="#PBsve426_S1" xml:id="PBsve426_u_11"><seg n="1" synch="#PBsve426_ol_0" type="overlap" xml:id="PBsve426_ol_1"><supplied reason="unintelligible"> xxx </supplied></seg></u>
            <u who="#PBsve426_S1" xml:id="PBsve426_u_12"> mhm </u>
            <u who="#PBsve426_S2" xml:id="PBsve426_u_13"> e<c type="lengthening"/>rm <pause dur="PT1S"/> you paid four hundred euro as <pause/> reservation <seg n="8" type="overlap" xml:id="PBsve426_ol_2"> deposit </seg> to our office<c function="fall" type="intonation"/><pause/></u>
            <u who="#PBsve426_S1" xml:id="PBsve426_u_14"><seg n="8" synch="#PBsve426_ol_2" type="overlap" xml:id="PBsve426_ol_3"> yes </seg></u>
            <u who="#PBsve426_S2" xml:id="PBsve426_u_15"> hh and did you pay <pause/> to <shift corresp="#PBsve426_s_2" feature="loud" new="f" xml:id="PBsve426_s_1"/><supplied reason="anonymization"> [PLACE1] </supplied><shift corresp="#PBsve426_s_1" feature="loud" new="neutral" xml:id="PBsve426_s_2"/> another deposit<c function="rise" type="intonation"/><pause dur="PT3S"/></u>
            <u who="#PBsve426_S1" xml:id="PBsve426_u_16"> e<c type="lengthening"/>rm <pause dur="PT1S"/> two thousand <pause/> three<c function="rise" type="intonation"/></u>
            <u who="#PBsve426_S2" xml:id="PBsve426_u_17"> mhm <pause/></u>
            <u who="#PBsve426_S1" xml:id="PBsve426_u_18"> october <pause/> i pay<c function="fall" type="intonation"/></u>
