      <optional>
        <attribute name="syl" ns="http://www.univie.ac.at/voice/ns/1.0">
          <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Specifies the approx. count of syllables</a:documentation>
          <ref name="data.count"/>
